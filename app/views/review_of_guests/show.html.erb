<div class="page-header">
  <h3>Review Of Guest #<%= @review_of_guest.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Rating</dt>
      <dd><%= @review_of_guest.rating %></dd>

      <dt>Content</dt>
      <dd><%= @review_of_guest.content %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/review_of_guests" class="btn btn-primary">
        Back
      </a>
      <a href="/review_of_guests/<%= @review_of_guest.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_review_of_guest/<%= @review_of_guest.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
